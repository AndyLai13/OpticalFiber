<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".MainActivity">

	<Button
		android:id="@+id/di1000"
		android:layout_width="@dimen/button_width"
		android:layout_height="@dimen/button_height"
		android:text="DI1000"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintBottom_toTopOf="@id/di5000"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintEnd_toStartOf="@id/di2000" />


	<Button
		android:id="@+id/di2000"
		android:layout_width="@dimen/button_width"
		android:layout_height="@dimen/button_height"
		android:text="DI2000"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintBottom_toTopOf="@id/undefined"
		app:layout_constraintStart_toEndOf="@id/di1000"
		app:layout_constraintEnd_toStartOf="@id/di3000" />

	<Button
		android:id="@+id/di3000"
		android:layout_width="@dimen/button_width"
		android:layout_height="@dimen/button_height"
		android:text="DI3000"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintBottom_toTopOf="@id/settings"
		app:layout_constraintStart_toEndOf="@id/di2000"
		app:layout_constraintEnd_toEndOf="parent"/>

	<Button
		android:id="@+id/di5000"
		android:layout_width="@dimen/button_width"
		android:layout_height="@dimen/button_height"
		android:text="DI5000"
		app:layout_constraintTop_toBottomOf="@id/di1000"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintEnd_toStartOf="@id/undefined"/>
	
	<Button
		android:id="@+id/undefined"
		android:layout_width="@dimen/button_width"
		android:layout_height="@dimen/button_height"
		android:text="UNDEFINED"
		app:layout_constraintTop_toBottomOf="@id/di2000"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintStart_toEndOf="@id/di5000"
		app:layout_constraintEnd_toStartOf="@id/settings"/>
	
	<Button
		android:id="@+id/settings"
		android:layout_width="@dimen/button_width"
		android:layout_height="@dimen/button_height"
		android:text="Settings"
		app:layout_constraintTop_toBottomOf="@id/di3000"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintStart_toEndOf="@id/undefined"
		app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>